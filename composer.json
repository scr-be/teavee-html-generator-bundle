{
    "name"        : "scr-be/teavee-html-component-generator-bundle",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://scr.be/teavee-html-component-generator-bundle",
    "description" : "A collection of objects/interfaces to simplify bundle setup, such as advanced bundle configuration, compiler passes, and deprecaton error handlers.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://scr.be/teavee-html-component-generator-bundle/issues",
        "source" : "https://scr.be/teavee-html-component-generator-bundle/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"   : [
        "teavee-html-component-generator-bundle", "scribe-teavee-html-component-generator-bundle", "bundle", "scr.be"
    ],
    "authors"    : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@build.fail",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php"                          : ">=5.6.4,~5.6|~7.0",
        "scr-be/wonka-bundle"          : "~0.2",
        "scr-be/wonka-library"         : "~0.1",
        "symfony/config"               : "~2.8|~3.0",
        "symfony/dependency-injection" : "~2.8|~3.0",
        "symfony/http-foundation"      : "~2.8|~3.0",
        "twig/twig"                    : "~1.23"
    },
    "require-dev" : {
        "twig/twig"                : "~1.23",
        "symfony/symfony"          : "~2.8|~3.0",
        "symfony/stopwatch"        : "~2.8|~3.0",
        "symfony/framework-bundle" : "~2.8|~3.0",
        "symfony/debug-bundle"     : "~2.8|~3.0",
        "symfony/monolog-bundle"   : "~2.8|~3.0",
        "symfony/phpunit-bridge"   : "~2.8|~3.0",
        "phpunit/phpunit"          : "~5.0",
        "codacy/coverage"          : "~1.0",
        "satooshi/php-coveralls"   : "~0.7"
    },
    "conflict" : {
        "scribe/mantle-bundle" : "<2.3.0"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\Teavee\\HtmlComponentGeneratorBundle" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : { "Scribe\\Teavee\\HtmlComponentGeneratorBundle\\Tests\\" : "tests/" }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "bin-dir" : "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}

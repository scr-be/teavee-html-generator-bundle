{
    "name"        : "scr-be/teavee-html-generator-bundle",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://scr.be/teavee-html-generator-bundle",
    "description" : "A framework for creating HTML components-buttons, icons, etc-with support for API-based calls or twig extensions.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://scr.be/teavee-html-generator-bundle/issues",
        "source" : "https://scr.be/teavee-html-generator-bundle/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"   : [
        "teavee-html-generator-bundle", "scribe-teavee-html-generator-bundle", "bundle", "scr.be"
    ],
    "authors"    : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@build.fail",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php"                                     : ">=5.6.4,~5.6|~7.0",
        "scr-be/wonka-library"                    : "~0.3",
        "scr-be/wonka-bundle"                     : "~0.3",
        "scr-be/teavee-object-cache-bundle"       : "~2.4",
        "scr-be/arthur-doctrine-library"          : "~0.3",
        "scr-be/arthur-doctrine-fixtures-bundle"  : "~0.4",
        "symfony/config"                          : "~2.8|~3.0",
        "symfony/dependency-injection"            : "~2.8|~3.0",
        "symfony/http-foundation"                 : "~2.8|~3.0",
        "symfony/twig-bundle"                     : "~2.8|~3.0",
        "twig/twig"                               : "~1.23"
    },
    "require-dev" : {
        "doctrine/doctrine-fixtures-bundle" : "~2.2",
        "symfony/symfony"                   : "~2.8|~3.0",
        "symfony/stopwatch"                 : "~2.8|~3.0",
        "symfony/framework-bundle"          : "~2.8|~3.0",
        "symfony/debug-bundle"              : "~2.8|~3.0",
        "symfony/monolog-bundle"            : "~2.8|~3.0",
        "symfony/phpunit-bridge"            : "~2.8|~3.0",
        "phpunit/phpunit"                   : "~5.0",
        "codacy/coverage"                   : "~1.0",
        "satooshi/php-coveralls"            : "~1.0"
    },
    "conflict" : {
        "scribe/mantle-bundle" : "<2.4.0"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\Teavee\\HtmlGeneratorBundle\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Scribe\\Teavee\\HtmlGeneratorBundle\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "bin-dir" : "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
